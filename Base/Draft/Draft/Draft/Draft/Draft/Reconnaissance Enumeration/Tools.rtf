\par\pard\plain \keep\li2160\fi-360\ltrch\loch \ls2\ilvl2{\listtext\f1\fs24\b0\i0 \loch\af1\hich\af1\dbch\af1\uc1\u9675\'3F\tab }{\f1\fs24\b0\i0 Ability to use any locally-configured network source address}
